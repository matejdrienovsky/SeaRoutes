<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="bg" prefHeight="253.0" prefWidth="275.0" stylesheets="@animation.css" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.searoutes.PopUpAfterAnimation">
   <children>
      <ImageView fitHeight="116.0" fitWidth="122.0" layoutX="80.0" layoutY="127.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@welcome.png" />
         </image>
      </ImageView>
      <Button id="buttonRating" fx:id="add" layoutX="87.0" layoutY="71.0" mnemonicParsing="false" onAction="#addRating" prefHeight="26.0" prefWidth="102.0" stylesheets="@animation.css" text="Rate the captain " />
      <ChoiceBox id="choice_box" fx:id="choiceRating" layoutX="94.0" layoutY="31.0" onMouseEntered="#initializeRatings" prefHeight="19.0" prefWidth="87.0" stylesheets="@animation.css" />
      <Label fx:id="label" alignment="CENTER" contentDisplay="CENTER" layoutX="15.0" layoutY="104.0" prefHeight="18.0" prefWidth="251.0" textAlignment="CENTER" textFill="RED" />
   </children>
</AnchorPane>
